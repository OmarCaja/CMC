Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{"Ej3", "[", "n_", "]"}], ":=", 
   RowBox[{"Module", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{
      "membrana", ",", " ", "aux", ",", " ", "randomNumber", ",", " ", 
       "result", ",", " ", "i"}], "}"}], ",", "\n", "\t", "\n", "\t", 
     RowBox[{
      RowBox[{"membrana", " ", "=", " ", 
       RowBox[{"{", 
        RowBox[{"a", ",", " ", "a"}], "}"}]}], ";", "\n", "\t", 
      RowBox[{"result", " ", "=", " ", 
       RowBox[{"{", "}"}]}], ";", "\n", "\t", "\n", "\t", 
      RowBox[{"For", "[", 
       RowBox[{
        RowBox[{"i", " ", "=", " ", "1"}], ",", " ", 
        RowBox[{"i", " ", "\[LessEqual]", " ", "n"}], ",", " ", 
        RowBox[{"i", "++"}], ",", "\n", "\t", "\n", "\t\t", 
        RowBox[{
         RowBox[{"aux", " ", "=", " ", 
          RowBox[{"{", "}"}]}], ";", "\n", "\t\t", "\n", "\t\t", 
         RowBox[{"While", "[", 
          RowBox[{
           RowBox[{
            RowBox[{"Length", "[", "membrana", "]"}], " ", ">", " ", "0"}], 
           ",", "\n", "\t\t", "\n", "\t\t\t", 
           RowBox[{
            RowBox[{"randomNumber", " ", "=", " ", 
             RowBox[{"RandomInteger", "[", "1", "]"}]}], ";", "\n", "\t\t", 
            "\n", "\t\t\t", 
            RowBox[{"If", "[", 
             RowBox[{
              RowBox[{"randomNumber", " ", "==", " ", "0"}], ",", "\n", 
              "\t\t", "\n", "\t\t\t\t", 
              RowBox[{
               RowBox[{"membrana", " ", "=", " ", 
                RowBox[{"Take", "[", 
                 RowBox[{"membrana", ",", " ", 
                  RowBox[{"-", 
                   RowBox[{"(", 
                    RowBox[{
                    RowBox[{"Length", "[", "membrana", "]"}], " ", "-", " ", 
                    "1"}], ")"}]}]}], "]"}]}], ";", "\n", "\t\t\t\t", 
               RowBox[{"AppendTo", "[", 
                RowBox[{"aux", ",", " ", "a"}], "]"}], ";", "\n", "\t\t\t\t", 
               
               RowBox[{"result", " ", "=", " ", 
                RowBox[{"Join", "[", 
                 RowBox[{"result", ",", " ", 
                  RowBox[{"{", 
                   RowBox[{"b", ",", "c", ",", "c"}], "}"}]}], "]"}]}], 
               ";"}]}], "\n", "\t\t\t", "]"}], ";", "\n", "\t\t\t", "\n", 
            "\t\t\t", 
            RowBox[{"If", "[", 
             RowBox[{
              RowBox[{
               RowBox[{"randomNumber", " ", "==", " ", "1"}], " ", "&&", " ", 
               
               RowBox[{
                RowBox[{"Length", "[", "membrana", "]"}], " ", 
                "\[GreaterEqual]", " ", "2"}]}], ",", "\n", "\t\t\t", "\n", 
              "\t\t\t\t", 
              RowBox[{
               RowBox[{"membrana", " ", "=", " ", 
                RowBox[{"Take", "[", 
                 RowBox[{"membrana", ",", " ", 
                  RowBox[{"-", 
                   RowBox[{"(", 
                    RowBox[{
                    RowBox[{"Length", "[", "membrana", "]"}], " ", "-", " ", 
                    "2"}], ")"}]}]}], "]"}]}], ";"}]}], "\n", "\t\t\t", "]"}],
             ";"}]}], "\n", "\t\t", "]"}], ";", "\n", "\t\t", 
         RowBox[{"membrana", " ", "=", " ", "aux"}], ";"}]}], "\n", "\t", 
       "]"}], ";", "\n", "\t", 
      RowBox[{"Return", "[", "result", "]"}], ";"}]}], "\n", "]"}]}], 
  ";"}]], "Code",
